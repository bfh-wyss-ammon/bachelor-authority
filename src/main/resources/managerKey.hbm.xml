<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="data.DbManagerKey" table="managerkey">
		<id name="managerKeyId" type="int" column="managerkeyid">
			<generator class="identity" />
		</id>
		<property name="Xg" type="hibernate.BigIntegerType">
			<column name="xg" />
		</property>
		<property name="p" type="hibernate.BigIntegerType">
			<column name="p" />
		</property>
		<property name="q" type="hibernate.BigIntegerType">
			<column name="q" />
		</property>
	</class>
</hibernate-mapping>