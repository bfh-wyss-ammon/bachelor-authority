<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="data.DbJoinSession" table="JoinSession">
		<id name="joinSessionId" type="int" column="joinsessionId">
			<generator class="identity" />
		</id>
		<many-to-one name="user" class="data.DbUser" column="userId" unique="true" not-null="true" lazy="false" />
		<property name="token">
			<column name="token" />
		</property>
		<property name="created">
			<column name="created" />
		</property>
	</class>
</hibernate-mapping>